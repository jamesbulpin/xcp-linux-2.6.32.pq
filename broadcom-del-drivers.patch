-	if (PCI_FUNC(bp->pdev->devfn) == 0) {
-		switch (strap) {
-		case 0x4:
-		case 0x5:
-		case 0x6:
-			bp->phy_flags |= BNX2_PHY_FLAG_SERDES;
-			return;
-		}
-	} else {
-		switch (strap) {
-		case 0x1:
-		case 0x2:
-		case 0x4:
-			bp->phy_flags |= BNX2_PHY_FLAG_SERDES;
-			return;
-		}
