-static inline u32 /*__attribute_pure__*/
-crc32c_le(u32 seed, unsigned char const *data, size_t length)
-{
-	__le32 crc = __cpu_to_le32(seed);
-
-	while (length--)
-		crc = crc32c_table[(crc ^ *data++) & 0xFFL] ^ (crc >> 8);
-
-	return __le32_to_cpu(crc);
-}
