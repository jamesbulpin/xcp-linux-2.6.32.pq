-	bnx2_shutdown_chip(bp);
-	bnx2_free_irq(bp);
-	bnx2_free_skbs(bp);
-	bnx2_free_mem(bp);
-	bp->link_up = 0;
-	netif_carrier_off(bp->dev);
-	bnx2_set_power_state(bp, PCI_D3hot);
-	return 0;
-}
