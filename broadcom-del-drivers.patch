-	struct mem_entry *mem_tbl;
-
-	if (CHIP_NUM(bp) == CHIP_NUM_5709)
-		mem_tbl = mem_tbl_5709;
-	else
-		mem_tbl = mem_tbl_5706;
-
-	for (i = 0; mem_tbl[i].offset != 0xffffffff; i++) {
-		if ((ret = bnx2_do_mem_test(bp, mem_tbl[i].offset,
