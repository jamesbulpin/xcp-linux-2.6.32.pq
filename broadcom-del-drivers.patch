-
-	pci_restore_state(pdev);
-	if (!netif_running(dev))
-		return 0;
-
-	bnx2_set_power_state(bp, PCI_D0);
-	netif_device_attach(dev);
-	bnx2_init_nic(bp, 1);
-	bnx2_netif_start(bp);
-	return 0;
-}
