-	if (!(bp->autoneg & AUTONEG_SPEED)) {
-		u32 new_bmcr;
-		int force_link_down = 0;
-
-		if (bp->req_line_speed == SPEED_2500) {
-			if (!bnx2_test_and_enable_2g5(bp))
-				force_link_down = 1;
-		} else if (bp->req_line_speed == SPEED_1000) {
-			if (bnx2_test_and_disable_2g5(bp))
