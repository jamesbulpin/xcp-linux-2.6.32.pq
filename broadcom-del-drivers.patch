-		mips_fw_file = FW_MIPS_FILE_06;
-		rv2p_fw_file = FW_RV2P_FILE_06;
-	}
-
-	rc = request_firmware(&bp->mips_firmware, mips_fw_file, &bp->pdev->dev);
