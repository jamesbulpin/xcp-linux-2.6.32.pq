-		{ 0x60000,  0x4000 },
-		{ 0xa0000,  0x3000 },
-		{ 0xe0000,  0x4000 },
-		{ 0x120000, 0x4000 },
-		{ 0x1a0000, 0x4000 },
-		{ 0xffffffff, 0    },
