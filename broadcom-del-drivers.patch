-static u32 bnx2_find_max_ring(u32 ring_size, u32 max_size)
-{
-	u32 max, num_rings = 1;
-
-	while (ring_size > MAX_RX_DESC_CNT) {
-		ring_size -= MAX_RX_DESC_CNT;
-		num_rings++;
-	}
-	/* round to next power of 2 */
-	max = max_size;
-	while ((max & num_rings) == 0)
-		max >>= 1;
-
-	if (num_rings != max)
-		max <<= 1;
-
-	return max;
-}
