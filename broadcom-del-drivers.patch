-		if (rc)
-			return rc;
-
-		cmd_flags = BNX2_NVM_COMMAND_LAST;
-		rc = bnx2_nvram_read_dword(bp, offset32, buf, cmd_flags);
-
-		memcpy(ret_buf, buf, 4 - extra);
-	}
